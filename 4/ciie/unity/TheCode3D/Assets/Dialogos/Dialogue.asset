  - ID: 22
    Speaker: 4
    SpeakerName: Anacleto
    PreviousMessage: 21
    NextMessage: 23
    MessageText: "Con este c\xF3digo China podr\xEDa hacer pedazos cualquier "
    Clip: {fileID: 0}
  - ID: 23
    Speaker: 4
    SpeakerName: Anacleto
    PreviousMessage: 22
    NextMessage: 24
