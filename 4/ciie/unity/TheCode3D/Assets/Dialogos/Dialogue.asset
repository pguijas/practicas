    Clip: {fileID: 0}
  - ID: 20
    Speaker: 8
    SpeakerName: "Hombre al tel\xE9fono"
    PreviousMessage: 19
    NextMessage: 21
    MessageText: 'Hahahahaha ... (Cuelga) '
    Clip: {fileID: 0}
  - ID: 21
    Speaker: 4
    SpeakerName: Anacleto
    PreviousMessage: 20
    NextMessage: 22
    MessageText: "No puede ser, he de ir a recuperar mi c\xF3digo ahora mismo."
