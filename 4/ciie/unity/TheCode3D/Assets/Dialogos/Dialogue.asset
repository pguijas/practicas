  - ID: 24
    Speaker: 4
    SpeakerName: Anacleto
    PreviousMessage: 23
    NextMessage: 26
    MessageText: "\xA1Podr\xEDa ser el fin del mundo!"
    Clip: {fileID: 0}
  - ID: 26
    Speaker: 4
    SpeakerName: Anacleto
    PreviousMessage: 24
    NextMessage: -1
